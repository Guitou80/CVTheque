@model ICollection<CVtheque.Web.Models.CompetenceVM>

@{
    ViewBag.Title = "Competences";
    Layout = null;
}

    @for (int i = 0; i < Model.Count(); i++)
    {

    <div class="row grid-striped highlightable" data-id="@Model.ElementAt(i).Id">

        <div class="col-11" >
            @Model.ElementAt(i).Label
        </div>
        <div class="col-1">
            <span class="green">
                <i class="fas fa-plus-square"></i>
            </span>
        </div>
       
    </div>

    }
