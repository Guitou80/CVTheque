@model ICollection<CVtheque.Web.Models.CompetenceVM>

@{
    ViewBag.Title = "Competences";
    Layout = null;
}

    @for (int i = 0; i < Model.Count(); i++)
    {

    <div class="row grid-striped">

        <div class="col-12" data-id="@Model.ElementAt(i).Id">
            @Model.ElementAt(i).Label
        </div>
       
    </div>

    }
