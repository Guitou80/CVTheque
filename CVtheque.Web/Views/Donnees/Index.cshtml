<h2> Mes données </h2>

<div style="width:800px;">

    <ul class="nav nav-tabs" role="tablist">
        <li class="nav-item">
            <a class="active nav-link active" href="#formations" role="tab" data-toggle="tab">formations</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="#competences" role="tab" data-toggle="tab">competences</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="#experiences" role="tab" data-toggle="tab">experiences</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="#langues" role="tab" data-toggle="tab">langues</a>
        </li>
    </ul>

    <!-- Tab panes -->
    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="formations">

            <div id="AffichageFormations">
                @Html.Action("Formations")
            </div>

            <div id="AffichageFormationForm">
                @Html.Action("AddOrEditFormation")
            </div>

        </div>
        <div role="tabpanel" class="tab-pane fade" id="competences">

            <div id="AffichageCompetences">
                @Html.Action("Competences")
            </div>

            <div id="AffichageCompetenceForm">
                @Html.Action("AddOrEditCompetence")
            </div>

        </div>
        <div role="tabpanel" class="tab-pane fade" id="experiences">

            <div id="AffichageExperiences">
                @Html.Action("Experiences")
            </div>

            <div id="AffichageExperienceForm">
                @Html.Action("AddOrEditExperience")
            </div>

        </div>
        <div role="tabpanel" class="tab-pane fade" id="langues">

            <div id="AffichageLangues">
                @Html.Action("Langues")
            </div>

            <div id="AffichageLangueForm">
                @Html.Action("AddOrEditLangue")
            </div>

        </div>
    </div>

</div>


    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")

    }
